<h2>Tasks for <%= @current_date %></h2>

<table class="table">
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th></th>
  </tr>
  <% @current_scheduled_tasks.each do |t| %>
    <tr>
      <td><%= t.title %></td>
      <td><%= t.description %></td>
      <td>Done Button</td>
    </tr>
  <% end %>
</table>
